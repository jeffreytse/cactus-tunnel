{
  "name": "cactus-tunnel",
  "version": "0.1.0",
  "description": "TCP tunnel over WebSocket and Browser",
  "main": "dist/src/index.js",
  "bin": {
    "cactus-tunnel": "dist/bin/cactus-tunnel.js"
  },
  "files": [
    "dist",
    "public",
    "views"
  ],
  "keywords": [
    "tcp",
    "tunnel",
    "websocket",
    "http"
  ],
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:cli": "npm run build && npm run cli",
    "build": "npm run lint && tsc && npm run bundle",
    "bundle": "browserify dist/src/bundle.js -o public/js/bundle.js",
    "serve": "node dist/src/index.js",
    "cli": "node dist/bin/cactus-tunnel.js",
    "lint": "eslint . --ext .ts",
    "test": "npm run build && jest dist/test/*.test.js",
    "prepublish": "npm run build",
    "prepublishOnly": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffreytse/cactus-tunnel.git"
  },
  "author": "Jeffrey Tse <jeffreytse.mail@gmail.com>",
  "homepage": "https://github.com/jeffreytse/cactus-tunnel#readme",
  "bugs": {
    "url": "https://github.com/jeffreytse/cactus-tunnel/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/jest": "^29.2.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^17.0.8",
    "@types/pump": "^1.1.1",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "axios": "^1.2.1",
    "babelify": "^10.0.0",
    "eslint": "^8.7.0",
    "gulp": "^4.0.2",
    "jest": "^29.3.1",
    "puppeteer": "^19.3.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "ejs": "^3.1.8",
    "express": "^4.17.3",
    "express-ws": "^5.0.2",
    "open": "^8.4.0",
    "pump": "^3.0.0",
    "websocket-stream": "^5.5.2",
    "winston": "^3.8.2",
    "ws": "^8.5.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "browserslist": "> 0.25%, not dead"
}
